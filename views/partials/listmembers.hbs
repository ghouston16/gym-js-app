{{#each members}}
<section class="ui stacked segment">
    <h2 class="ui header">
        {{firstName}} {{lastName}}
    </h2>
    <p> E-mail: {{email}} </p>
    <p>
        <a href="/member/{{id}}" class="ui icon button">
            <i class="icon folder open"></i>
        </a>
    <td>
        <a href="/member/updatemember/{{id}}" class="ui icon button">
            <i class="pencil alternate icon"></i>
        </a>
    </td>
    <a href="/dashboard/deletemember/{{id}}" class="ui icon button">
        <i class="icon trash"></i>
    </a>
</section>
{{/each}}
